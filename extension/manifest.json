{
  "name": "ws extension",
  "version": "0.0.0",
  "description": "Using websockets in a Chrome Extension",
  "manifest_version": 3,
  "author": "kllarena07",
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.youtube.com/watch?v=*"]
    }
  ],
  "permissions": ["storage", "webNavigation"]
}
